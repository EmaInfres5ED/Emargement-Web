<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ema\RgementBundle\Entity\ParticipationAbsence" table="participation_absence">
    <indexes>
      <index name="fk_participation_abs_idx" columns="id_participation"/>
      <index name="fk_abs_participation_idx" columns="id_absence"/>
    </indexes>
    <id name="idParticipationAbsence" type="integer" column="id_participation_absence">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idAbsence" target-entity="Absence">
      <join-columns>
        <join-column name="id_absence" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idParticipation" target-entity="Participation">
      <join-columns>
        <join-column name="id_participation" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
