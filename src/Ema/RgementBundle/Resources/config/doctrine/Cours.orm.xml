<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ema\RgementBundle\Entity\Cours" table="cours">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelle" type="string" column="libelle" length="100" nullable="false"/>
    <field name="professeur" type="string" column="professeur" length="100" nullable="true"/>
    <field name="date" type="datetime" column="date" nullable="false"/>
    <field name="heureDebut" type="string" column="heure_debut" length="10" nullable="true"/>
    <field name="heureFin" type="string" column="heure_fin" length="10" nullable="true"/>
    <field name="idCybema" type="integer" column="id_cybema" nullable="true"/>
    <field name="salle" type="string" column="salle" length="45" nullable="true"/>
  </entity>
</doctrine-mapping>
