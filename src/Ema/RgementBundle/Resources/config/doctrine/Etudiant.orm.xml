<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ema\RgementBundle\Entity\Etudiant" table="etudiant">
    <indexes>
      <index name="fk_etudiant_promo_idx" columns="id_promo"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="100" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="100" nullable="false"/>
    <field name="email" type="string" column="email" length="100" nullable="false"/>
    <many-to-one field="idPromo" target-entity="Promotion">
      <join-columns>
        <join-column name="id_promo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
