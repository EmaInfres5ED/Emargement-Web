<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ema\RgementBundle\Entity\Presence" table="presence">
    <indexes>
      <index name="fk_presence_participation_idx" columns="id_participation"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="signature" type="string" column="signature" length="150" nullable="true"/>
    <many-to-one field="idParticipation" target-entity="Participation">
      <join-columns>
        <join-column name="id_participation" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
