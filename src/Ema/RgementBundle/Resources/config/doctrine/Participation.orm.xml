<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ema\RgementBundle\Entity\Participation" table="participation">
    <indexes>
      <index name="fk_participation_cours_idx" columns="id_cours"/>
      <index name="fk_participation_etudiant_idx" columns="id_etudiant"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idEtudiant" target-entity="Etudiant">
      <join-columns>
        <join-column name="id_etudiant" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCours" target-entity="Cours">
      <join-columns>
        <join-column name="id_cours" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
